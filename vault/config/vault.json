{
    "storage": {
        "file": {
            "path": "/vault/file"
        }
    },
    "listener": {
        "tcp": {
            "address": "0.0.0.0:8200",
            "tls_disable": true
        }
    },
    "ui": true,
    "disable_mlock": true,
    "log_file": "/vault/logs/vault.json",
    "log_level": "info",
    "log_format": "json",
    "log_rotate_duration": "24h",
    "cache_size": "32768",
    "telemetry": {
        "disable_hostname": true,
        "prometheus_retention_time": "12h"
    }
}